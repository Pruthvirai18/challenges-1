#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i = 4;
    double d = 4.0;
    char s[] = "HackerRank ";
    int i1;
    double i2;
    char s2[100];
    scanf("%d",&i1);
    scanf("%lf",&i2);
    scanf("%*[\n] %[^\n]", s2); 
    printf("%d\n",i+i1);
    printf("%0.1lf\n",d+i2);
    printf("%s%s",s,s2);
    
    return 0;
}
